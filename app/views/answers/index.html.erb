<table>
  <tr>
    <th>Question</th>
    <th>Your Answer</th>
    <th>Answer</th>
    <th>Result</th>
  </tr>
<% @answers.each do |answer| %>
  <tr>
    <td><%= answer.m_word.question_word %></td>
    <td><%= answer.characters %></td>
    <td><%= answer.m_word.answer_word %></td>
    <td><%= answer.correct %></td>
  </tr>
<% end %>
</table>
